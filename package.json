{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@skyscanner/backpack-react-scripts": "8.0.2",
    "axios": "^0.21.1",
    "bpk-component-button": "^3.2.63",
    "bpk-component-code": "^2.0.84",
    "bpk-component-icon": "^8.2.23",
    "bpk-component-text": "^3.1.19",
    "bpk-mixins": "^20.1.1",
    "bpk-stylesheets": "^6.0.51",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint",
    "cypress:open": "cypress open",
    "test:a11y": "cypress run -s cypress/integration/a11y/**/*.spec.js -b chrome --headless",
    "test:a11y:ci": "npm run test:a11y -- -q --parallel --record --group a11y",
    "test:e2e": "cypress run -s cypress/integration/e2e/**/*.spec.js -b chrome --headless",
    "test:e2e:ci": "npm run test:e2e -- -q --parallel --record --group e2e",
    "test:perf": "cypress run -s cypress/integration/perf/**/*.spec.js -b chrome --headless",
    "test:perf:ci": "npm run test:perf -- -q --parallel --record --group perf",
    "test:visual": "cypress run -s cypress/integration/visual/**/*.spec.js -b chrome --headless",
    "test:visual:ci": "npm run test:visual -- -q --parallel --record --group visual",
    "serve": "serve --single --no-clipboard -l 5000 ./build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@applitools/eyes-cypress": "^3.22.1",
    "@testing-library/react": "^11.2.6",
    "cypress": "^7.0.1",
    "cypress-audit": "^0.2.3",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "serve": "^11.3.2"
  }
}
